<h1 style="color: #eb1c23; font-family: Verdana">User</h1>
<div class="user-contentborder">
  <div class="form-row">
    <div class="form-group col-md-3">
      <label for="product-name">Username</label>
      <input
        type="text"
        class="form-control"
        id="username"
        name="username"
        required
      />
    </div>
    <div class="form-group col-md-3">
      <label for="last-name">Email</label>
      <input
        type="text"
        class="form-control"
        id="email"
        name="email"
        required
      />
    </div>
    <div class="form-group col-md-3">
      <label for="email">Password</label>
      <input
        type="email"
        class="form-control"
        id="password"
        name="password"
        required
      />
    </div>
    <div class="form-group col-md-6">
      <label for="phone-number">Full Name</label>
      <input
        type="tel"
        class="form-control"
        id="fullname"
        name="fullname"
        required
      />
    </div>
    <div class="form-group col-md-2">
      <label for="country">Country</label>
      <input
        type="text"
        class="form-control"
        id="country"
        name="country"
        required
      />

      <label for="state">Active</label>
      <input
        type="checkbox"
        class="form-check-input"
        id="active"
        name="active"
        value="0"
        style="margin-left: 10px; height: 30px; width: 30px"
        required
      />
      <div>
        <label for="state">Admin</label>
        <input
          type="checkbox"
          class="form-check-input"
          id="admin"
          name="admin"
          style="margin-left: 5px; height: 30px; width: 30px"
          required
        />
      </div>
    </div>
  </div>
  <div>
    <button type="submit" id="add" name="add">
      Add &nbsp;<i class="bx bx-message-square-add"></i>
    </button>
  </div>
</div>

&nbsp;
<div class="user-contentborder">
  <table id="user-table" class="table table-bordered">
    <thead class="">
      <th scope="col-md-4">User ID</th>
      <th scope="col-md-4">Username</th>
      <th scope="col-md-4">Email</th>
      <th scope="col-md-4">Password</th>
      <th scope="col-md-4">Full Name</th>
      <th scope="col-md-4">Registration Date</th>
      <th scope="col-md-4">Last Login</th>
      <th scope="col-md-4">Active</th>
      <th scope="col-md-4">Admin</th>
      <th>Action</th>
    </thead>
    <tbody></tbody>
  </table>
</div>
